{
    "version": "1.0.0",
    "manifest_version": 3,
    "name": "Advance Bookmark",
    "permissions":[
        "bookmarks"
    ],
    "description": "This is a Chrome extension built with React",
    "action": {
        "default_popup": "index.html"
      },
      "icons":{
        "128":"logo.png"
      },
      "commands": {
        "_execute_action": {
          "suggested_key": {
            "windows": "Ctrl+Shift+Q",
            "mac": "MacCtrl+Shift+Q"
          },
          "description": "Open Your Extension"
        }
      }
}
